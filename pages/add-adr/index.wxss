page{
  width: 750rpx;
  height:auto;
  background-color: #f5f5f5;
  font-size: 26rpx;
  color:#333;
}
.mains_big{
  width: 90%;
  height: 700rpx;
  background-color: #ffffff;
  margin: 20rpx auto;
  display: flex;
  /* position: fixed; */
  position: relative;
  flex-direction: column;
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.mains_small{
  width: 100%;
  height: 100rpx;
  display: flex;
  margin-top:10rpx;
  line-height: 100rpx;
}
.mains_small_1{
  width: 100%;
  height: 140rpx;
  display: flex;
  margin-top:10rpx;
}
.main_right_1{
  width: 120rpx;
  height: 120rpx;
  display: flex;
  position:absolute;
  border-radius: 120rpx;
  top: 20rpx;
  right: 20rpx;
}
.image{
  width: 100%;
  height: 100%;
  display:block;
  overflow:hidden;
  border-radius: 120rpx;
}
.main_top{
  width: 100%;
  height: 4rpx;
  background-color: #f5f5f5;
  display: flex;
  /* position:absolute; */
  top: 0;

}
.main_left{
  width: 15%;
  height: 100%;
  display: flex;
  /* justify-content:center; */
  align-items:center;
  font-size: 28rpx;
  color: #333333;
}
.main_right{
  font-size: 28rpx;
  display: flex;
  justify-content:center;
  align-items:center;
  font-size: 26rpx;
  color: #888888;
  position:absolute;
  right: 50rpx;
}
.main_right2{
  font-size: 36rpx;
  display: flex;
  justify-content:center;
  align-items:center;
  font-size: 26rpx;
  color: #888888;
  position:absolute;
  right: 20rpx;
}
.picker{
  background-color:transparent;
}