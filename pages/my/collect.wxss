page{
  /* background:-webkit-linear-gradient(top,#f9e3b8,#e0e0e0); */
  background-color: #e0e0e0;
}
.page-box{
  width: 100%;
}
.collect-box{
  width: 96%;
  height: 232rpx;
  margin: 20rpx auto;
  background-color: #ffffff;
  border-radius: 10rpx;
  padding-top: 10rpx;
  display: flex;
  flex-direction: row;
}
.box-image{
  width: 220rpx;
  height: 220rpx;
  background-color: #d8d8d8;
  margin-left: 10rpx;
  border-radius: 16rpx;
}
.box-image image{
  width: 100%;
  height: 100%;
  border-radius: 16rpx;
}
.box-texts{
  width: 460rpx;
  height: 220rpx;
  border-radius: 16rpx;
  margin-left: 20rpx;
}
.text01{
  height: 96rpx;
  font-size: 34rpx;
  color:#222222;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-wrap: break-word;
  white-space: normal !important;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.text02{
  width: 180rpx;
  height: 40rpx;
  background-color: #f9e3b8;
  color: #d26500;
  font-size: 24rpx;
  line-height: 40rpx;
  border-radius: 100rpx;
  text-align: center;
  margin-top: 10rpx;
}
.text03{
  height: 60rpx;
  display: flex;
  flex-direction: row;
  margin-top: 20rpx;
  line-height: 60rpx;
}
.text03-1{
  width: 250rpx;
  font-size: 32rpx;
  font-weight: bold;
  color:#d11212;
}
